{
    "name": "vankosoft\/application",
    "type": "library",
    "version": "v1.7.30",
    "description": "VankoSoft Symfony Application",
    "keywords": [
        "symfony",
        "sylius",
        "application",
        "distribution"
    ],
    "homepage": "https:\/\/github.com\/iatanasov77\/vs-application-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan I. Atanasov",
            "email": "i.atanasov77@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony\/monolog-bundle": "^3.7",
        "symfony\/webpack-encore-bundle": "^1.11",
        "symfony\/templating": "^5.2",
        "symfony\/security-bundle": "^4.4 || ^5.2",
        "symfony\/mailer": "^4.4 || ^5.2",
        "twig\/extra-bundle": "^3.3",
        "sensio\/framework-extra-bundle": "^6.1",
        "symfonycasts\/verify-email-bundle": "^1.5",
        "symfonycasts\/reset-password-bundle": "^1.8",
        "lexik\/jwt-authentication-bundle": "^2.11",
        "doctrine\/doctrine-migrations-bundle": "^3.1",
        "stof\/doctrine-extensions-bundle": "^1.6",
        "doctrine\/doctrine-fixtures-bundle": "^3.4",
        "knplabs\/knp-menu-bundle": "^3.1",
        "knplabs\/knp-paginator-bundle": "^5.5",
        "knplabs\/knp-time-bundle": "^1.16",
        "knplabs\/knp-gaufrette-bundle": "^0.7",
        "friendsofsymfony\/ckeditor-bundle": "^2.0",
        "liip\/imagine-bundle": "^2.6",
        "artgris\/filemanager-bundle": "^2.2",
        "spe\/filesize-extension-bundle": "^2.0",
        "sylius\/resource-bundle": "^1.8",
        "sylius\/taxonomy": "^1.9",
        "sylius\/locale": "^1.9",
        "sylius\/theme-bundle": "^2.2",
        "sylius-labs\/doctrine-migrations-extra-bundle": "^0.1.4",
        "sylius\/fixtures-bundle": "^1.7",
        "friendsofsymfony\/jsrouting-bundle": "^2.8",
        "willdurand\/js-translation-bundle": "^5.0",
        "hackzilla\/password-generator-bundle": "^4.1"
    },
    "replace": {
        "vankosoft\/application-bundle": "self.version",
        "vankosoft\/application-instalator-bundle": "self.version",
        "vankosoft\/users-bundle": "self.version",
        "vankosoft\/cms-bundle": "self.version"
    },
    "suggest": {
        "symfony\/debug-bundle": "Allows developement debugging",
        "symfony\/maker-bundle": "Standard symfony console maker tools",
        "symplify\/changelog-linker": "Allows generation of nice CHANGELOG.md file"
    },
    "conflict": {
        "doctrine\/doctrine-bundle": "<1.3",
        "symfony\/doctrine-bridge": "<2.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "autoload": {
        "psr-4": {
            "Vankosoft\\ApplicationInstalatorBundle\\": "src\/Vankosoft\/ApplicationInstalatorBundle\/",
            "Vankosoft\\ApplicationBundle\\": "src\/Vankosoft\/ApplicationBundle\/",
            "Vankosoft\\UsersBundle\\": "src\/Vankosoft\/UsersBundle\/",
            "Vankosoft\\CmsBundle\\": "src\/Vankosoft\/CmsBundle\/"
        }
    }
}