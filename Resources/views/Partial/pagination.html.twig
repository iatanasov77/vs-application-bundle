{%- extends '@Pagerfanta/default.html.twig' -%}

{%- block pager -%}
    {#
    Page: {$page}<br />
    Pages Count: {$pagesNum}<br />
    Items Count: {$totalItemsCount}<br /><br />
    #}
    <!-- VsPager BEGIN -->

    <nav aria-label="Page navigation example" class="mx-auto d-flex justify-content-center">
        <ul class="pagination">
            <li class="page-item">
                <a class="page-link" href="#" aria-label="Previous">
                    <span aria-hidden="true">«</span><span class="sr-only">Previous</span>      </a>
            </li>
            <li class="page-item active"><a class="page-link" href="#">1</a></li>
            <li class="page-item"><a class="page-link" href="#">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item">
                <a class="page-link" href="#" aria-label="Next"><span aria-hidden="true">»</span>
                   <span class="sr-only">Next</span></a>
            </li>
        </ul>
    </nav>
    
    
    <!-- VsPager END -->
{%- endblock pager -%}
